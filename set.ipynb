   "id": "db5280da",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "{'union': {1, 2, 3, 4, 5, 6}, 'intersection': {3, 4}, 'difference': {1, 2}}\n"
     ]
    }
   ],
   "source": [
    "def set_operations(list1, list2):\n",
    "    s1, s2 = set(list1), set(list2)\n",
    "    return {\n",
    "        \"union\": s1 | s2,\n",
    "        \"intersection\": s1 & s2,\n",
    "        \"difference\": s1 - s2\n",
    "    }\n",
    "\n",
    "print(set_operations([1,2,3,4], [3,4,5,6]))"
   ]
  }
 ],
